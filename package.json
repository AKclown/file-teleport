{
	"name": "file-teleport",
	"displayName": "File Teleport",
	"description": "The contents of one file are transferred to other files",
	"repository": "https://github.com/AKclown/file-teleport.git",
	"version": "0.0.1",
	"publisher": "AKclown",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:file.teleport.openFile",
		"onCommand:file.teleport.default",
		"onCommand:file.teleport.insert",
		"onCommand:file.teleport.replace"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"category": "File Teleport",
				"command": "file.teleport.openFile",
				"title": "Select for Content Transfer"
			},
			{
				"category": "File Teleport",
				"command": "file.teleport.default",
				"title": "Default"
			},
			{
				"category": "File Teleport",
				"command": "file.teleport.insert",
				"title": "Insert Text"
			},
			{
				"category": "File Teleport",
				"command": "file.teleport.replace",
				"title": "Replace Text"
			}
		],
		"configuration": [
			{
				"title": "FileTeleport",
				"properties": {
					"Multiple files": {
						"type": "array",
						"items": {
							"type": "string"
						},
						"default": [],
						"description": "配置多个文件路径(当前选中文件目录下的同级子文件,不存在则创建)",
						"scope": "window"
					}
				}
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "file.teleport.openFile",
					"group": "3_compare",
					"when": "!explorerResourceIsFolder"
				}
			],
			"editor/context": [
				{
					"submenu": "file.teleport.editor.context",
					"group": "1_modification",
					"when": "editorHasSelection"
				}
			],
			"file.teleport.editor.context": [
				{
					"command": "file.teleport.default",
					"group": "1_modification",
					"when": "editorTextFocus"
				},
				{
					"command": "file.teleport.insert",
					"group": "1_modification",
					"when": "editorTextFocus"
				},
				{
					"command": "file.teleport.replace",
					"group": "1_modification@1",
					"when": "editorTextFocus"
				}
			]
		},
		"submenus": [
			{
				"id": "file.teleport.editor.context",
				"label": "File Content Transfer"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.15.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	}
}